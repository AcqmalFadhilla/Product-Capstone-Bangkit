<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.dashboard.fragment.explore.ExploreFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <include
                android:id="@+id/itemSearchBanner"
                layout="@layout/item_searching_banner" />

            <TextView
                android:id="@+id/tv_message_not_found_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Data tidak ditemukan!"
                android:layout_margin="20dp"
                android:layout_gravity="center"/>

            <include
                android:id="@+id/listSearchPlaceLayout"
                layout="@layout/layout_section_list_item"/>
        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>